    "workspaceFolder": "/home/vscode/debian",
    "features": {
        "ghcr.io/yhs88a/devcontainer/common-utils:1@sha256:c7569f0ed89459bb27b5a570905670da153a9d352bab8c916961fddb2cd17d7a": {},
